<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="-Infinity" prefWidth="400.0" spacing="7.0" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.printf.educake.controller.base.ErrorDialogController">
   <children>
      <Label fx:id="messageLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="message" textAlignment="LEFT" wrapText="true">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="detailsLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="details" textAlignment="LEFT" wrapText="true">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <HBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" maxWidth="+Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <JFXButton fx:id="okButton" buttonType="RAISED" minWidth="80.0" mnemonicParsing="false" style="-fx-background-color: #AB47BC;" text="Ok" textFill="WHITE" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="14.0" />
               </HBox.margin>
            </JFXButton>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
