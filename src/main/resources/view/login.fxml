<?xml version="1.0" encoding="UTF-8"?>


<?import io.github.printf.educake.util.generators.MaskField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.printf.educake.controller.LoginController">
   <children>
      <TextField maxWidth="1.7976931348623157E308" prefWidth="200.0" VBox.vgrow="ALWAYS" fx:id = "user"/>
      <MaskField mask="(DD)DDDD-DDDD" fx:id = "tel"/>
      <PasswordField maxWidth="1.7976931348623157E308" prefWidth="200.0" VBox.vgrow="ALWAYS" fx:id = "pass"/>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Button contentDisplay="CENTER" mnemonicParsing="false" text="Sair" HBox.hgrow="ALWAYS" />
            <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#tryLogin" text="Entrar" HBox.hgrow="ALWAYS" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</VBox>
